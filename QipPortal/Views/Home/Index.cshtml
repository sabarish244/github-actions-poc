    @section Scripts {
        <script type="text/javascript">
            $(function () {
                var Dirreportid =0;
                $('[data-toggle="popover"]').popover();

                Dirreportid =@ViewBag.reportId;


                $("#PowerReport").hide();

                //As per Log file
                $("#sellogfile").change(function () {
                    // jQuery
                    var selectedVal = $(this).val();
                    $('#hdnLoggerID').val(selectedVal);
                    $('.subcategory-dropdown-report').each(function () {
                        var allli = $(this).attr("onclick");

                        $(this).attr("onclick", allli.substring(0, allli.length - 2) + "Logfile=" + $('#hdnLoggerID').val() + "';");
                    });
                });

                if (Dirreportid != 0) {
                    ClicktoQIPtab();
                    Dirreportid = '#' + Dirreportid;
                    $('#VersionModal').hide();
                    //remove the backdrop
                    $('.modal-backdrop').remove();
                    $(''+ Dirreportid +'').click();
                }

                $('.modal').on('hidden.bs.modal', function (e) {
                    stopVideoForClosedModal(this);
                });
            });

            /* Script to hide the Side bar menu click open app or Report */
            function SwitchViews(newViewId, RID, FavouriteID, PojectID) {
                var viewID = [];
                try {

                    /*Loader show*/
                    $(".landingPageMain").addClass("d-none");
                    $("#LandingPageLoader").removeClass("d-none");


                    viewID = newViewId.split(" ");
                    viewID = viewID[0].replace(".", "");
                    viewID = viewID.replace(")", "");
                    viewID = viewID.replace("(", "");
                    div_appname = viewID.replace("-", "");
                    div_appFullname = newViewId;
                    $("#welcomepages").css('display', 'none');
                    $("#divXDKLaunch").css('display', 'none');
                    $(".xdk-container").css('display', 'none');
                    $('.addfavorites').removeClass("activeFav");
                    $("#divhelp").css('display', 'none');
                    $("#divUser_Guide").css('display', 'none');
                    // IT's checking Favorite or not
                    if ($.inArray(RID, AddRemoveFav) > -1) {
                        $(".addfavorites").parent().attr("title", "Remove Favorite");
                        $('.addfavorites').addClass("activeFav");
                    }
                    else {
                        $(".addfavorites").parent().attr("title", "Add Favorite");
                        $('.addfavorites').removeClass("activeFav");
                    }
                    /*checking is the coming app created or not*/
                    if ($(`.landingPageMain ${"#div" + div_appname}`).length) {
                        $("#div" + div_appname).css('display', 'block');
                        /*Remove app not found screen*/
                        $("#divAppNotFound").addClass("d-none");
                    } else {
                        /*showing app not found screen*/
                        $("#divAppNotFound").removeClass("d-none");
                    }

                    /*Loader remove*/
                    $("#LandingPageLoader").addClass("d-none");
                    $(".landingPageMain").removeClass("d-none");
                }
                catch (ex) {
                    alert(ex);
                }
            }

            /*Logging function*/
            $("#loggingIcon").click(function () {
                $("#loggingData").addClass("active")
            });

            $("#loggingDataClose").click(function () {
                $("#loggingData").removeClass("active")
            });

            /*Select Email ID in clipboard*/
            $(document).on('click', '.copyEmailId', function (e) {
                const copiedText = $(this).parent().find("span").text();
                var $temp = $("<input>");
                $("body").append($temp);
                $temp.val(copiedText).select();
                document.execCommand("copy");
                /*Copied state*/
                $(this).addClass("copiedText");
                setTimeout(() => {
                    $(this).removeClass("copiedText");
                }, 2000)
                $temp.remove();
            });

            /*Version Page*/
            $(document).on('click', '.VersionTimelineContentTitle', function (e) {
                // adding class clicked parent element
                $(this).closest(".VersionTimelineChild").toggleClass("active");
            });

            @* Script to hide the Side bar menu click like XDK Lanch App *@
            function Allapplanch(currentAppName, xdkAppName) {
                    try {
                        $("#" + currentAppName).css('display', 'none');
                        $("#" + xdkAppName).css('display', 'block');
                    } catch (ex) {
                        alert(ex);
                    }

            }

            // Site map indivual open function
            $(document).on('click', '.siteMapAppDetailsTitle', function (e) {
                // adding class clicked parent element
                $(this).closest(".siteMapHeadCat").toggleClass("active");
                // In this condition for css updated. I need last categories has clicked or not
                if ($(this).hasClass("lastCat-SDLC")) {
                    $(this).closest(".siteMapHead").toggleClass("active-SDLC");
                } else if ($(this).hasClass("lastCat-MBU")) {
                    $(this).closest(".siteMapHead").toggleClass("active-MBU");
                } else if ($(this).hasClass("lastCat-CBU")) {
                    $(this).closest(".siteMapHead").toggleClass("active-CBU");
                }
                // checking all the categories are opened or not. If it all are opened we update Expand button into Collapse
                const mapActiveClass = document.querySelectorAll(".siteMapHeadCat");
                var count = 0;
                for (var i = 0; i < mapActiveClass.length; i++) {
                    // checking all the categories are opened or not.
                    if (mapActiveClass[i].classList.contains('active')) {
                        count++;
                        // checking add are opened
                        if (count == 13) {
                            // update inner text of the button
                            $(".siteMapECAll").find(".p-text").html("Collapse All");
                            // adding active state to button
                            $(".siteMapECAll").addClass("active");
                        } else {
                            // update inner text of the button
                            $(".siteMapECAll").find(".p-text").html("Expand All");
                            // removing active state to button
                            $(".siteMapECAll").removeClass("active");
                        }
                    }
                }
            });

            // Expand and Collapse function
            $(document).on('click', '.siteMapECAll', function () {
                // adding active state to button
                $(this).toggleClass("active");
                // Checking axtive state and update inner text of the button
                if ($(this).hasClass("active")) {
                    // update inner text of the button
                    $(this).find(".p-text").html("Collapse All");
                    // Adding active class for all the categories
                    $(".siteMapHeadCat").addClass("active");
                    // Add active state in Last category
                    $(".siteMapHead-SDLC").addClass("active-SDLC");
                    $(".siteMapHead-MBU").addClass("active-MBU");
                    $(".siteMapHead-CBU").addClass("active-CBU");
                } else {
                    // update inner text of the button
                    $(this).find(".p-text").html("Expand All");
                    // Removing active class for all the categories
                    $(".siteMapHeadCat").removeClass("active");
                    // Remove active state in Last category
                    $(".siteMapHead-SDLC").removeClass("active-SDLC");
                    $(".siteMapHead-MBU").removeClass("active-MBU");
                    $(".siteMapHead-CBU").removeClass("active-CBU");
                }
            });
        </script>

    }


