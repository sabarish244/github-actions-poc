@using Microsoft.AspNetCore.Http

<!DOCTYPE html>
<html lang="en">
<head>


    <meta charset="UTF-8">
    <title>X Platform</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script src="https://kit.fontawesome.com/925dfd6778.js"></script>

    <script src="~/js/custom.js"></script>

    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/css/main.css" />
    <link rel="stylesheet" href="~/css/dore.light.purple.css" />
    <link rel="stylesheet" href="~/css/custom.css" />
    <link rel="stylesheet" href="~/css/jquery.contextMenu.css" />
    <link rel="stylesheet" type="text/css" href="//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.css" />
    <link href="~/css/style_M.css" rel="stylesheet" />
    <link rel="icon" type="image/png" href="~/favicon.ico">

    <script type="text/javascript">
        /*Page Loader*/
        $(window).on('load', function () {
            $('.pageLoder').addClass("d-none");
            $("body").css("overflow-y", "auto");
        })
    </script>
</head>

<body id="app-container" class="menu-sub-hidden">
    <input type="hidden" id="ReportID" name="ReportID" value="0">
    <input type="hidden" id="hdnLoggerID" name="hdnLoggerID" value="0">
    <input type="hidden" id="alreadyFevID" name="alreadyFevID" value="0">
    @*Page Loader*@
    <div class="pageLoder">
        <div class="Xlogo">
            <span></span>
            <span></span>
            <span></span>
            <span></span>
            X
        </div>
    </div>

    @*== Welcome Modal ===*@
    <div class="modal fade" id="VersionModal" tabindex="-1" role="dialog" data-backdrop="static" data-keyboard="false" aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered" style="max-width: 800px;">
            <div class="modal-content" style="border-radius: 10px; overflow: hidden">
                <div class="modal-header px-sm-5 px-3 py-3 p-0 m-0 d-block" style="height: auto">
                    <div class="d-flex w-100 justify-content-between align-items-center">
                        <div class="p-text">
                            Welcome
                            <br />
                            <div class="h1-text">
                                @User.Identity.Name
                            </div>
                        </div>
                        <div class="text-right p-text">
                            Current Version <strong>@ViewBag.VersionNo</strong>
                            <div class="mt-2">
                                Released on <strong>02 Aug 2022</strong>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-body d-block px-sm-5 px-3 py-3 p-0 m-0">
                    <div class="d-flex align-items-center justify-content-between">
                        <div class="h1-text">
                            What's new in X Platform?
                        </div>
                        <div>
                            <button class="d-flex align-items-center cursor-pointer" data-dismiss="modal" onclick="ShowNonResourceItem('help');">
                                <div>
                                    <img alt="version-file" src="../../img/icons/Version-File.svg">
                                </div>
                                <div class="p-text">
                                    <strong><span>Version history</span></strong>
                                </div>
                            </button>
                        </div>
                    </div>
                    <div class="p-text mt-2">
                        See the latest updates, new features, and tools, and get the most of your X Platform experience.
                    </div>
                    <div class="ml-3 mt-3">
                        <div class="d-flex mt-2">
                            <div>
                                <img alt="version" src="../../img/icons/Version-Add.svg">
                            </div>
                            <div class="ml-2">
                                <div class="h1-text">
                                    What’s New?
                                </div>
                                <div class="p-text">
                                    <ul class="ml-3">
                                        <li>Added SDLC, MBU, and CBU views in menu and sitemap as default for all users</li>
                                        <li>Land directly on QIP Tab, when the user doesn't have the access to apps</li>
                                        <li>Sidebar collapse feature</li>
                                        <li>Added TDC, DIARY and DevOps Maturity Analyzer App</li>
                                        <li>Included project name under the app names for sidebar favorites and search results</li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer d-block px-sm-5 pb-3 pt-0 py-3 px-3 p-0 m-0">
                    <div class="d-flex justify-content-between align-items-center pb-3">
                        <div class="p-text">
                            <i>You can view the same details in the help tab</i>
                        </div>
                        <div>
                            <button type="button" class="enterPopupbtn" data-dismiss="modal">Continue</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

