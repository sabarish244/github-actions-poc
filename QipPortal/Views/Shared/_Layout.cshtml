    <script src="~/js/resizable.js"></script>

    <script>
        /*Sidebar menu height*/
        function resetHeight() {
            const sideBarMenus = document.querySelectorAll(".sidebar-menu .container-menu");
            // reset the Sidebar menu height to that of the inner browser
            for (var i = 0; i < sideBarMenus.length; i++) {
                sideBarMenus[i].style.height = (window.innerHeight - 230) + "px";
            }
        }
        // reset the height whenever the window's resized
        window.addEventListener("resize", resetHeight, false);
        // called to initially set the height.
        resetHeight();

        // Sticky navbar while page scrolls
        $(window).scroll(function () {
            var sticky = $('.navBarhead'),
                scroll = $(window).scrollTop();

            if (scroll >= 10) {
                $(".mobNavLogo").addClass("active");
                $(".navBarUserIcon").addClass("active");
                sticky.addClass('active');
                $(".navBarSearchBtn").removeClass("pt-4");
            }
            else {
                $(".mobNavLogo").removeClass("active")
                $(".navBarUserIcon").removeClass("active");
                sticky.removeClass('active');
                $(".navBarSearchBtn").addClass("pt-4");
            }
        });
    </script>

    <script>

        //This script is to get all the strategies from Reports Controller and pass the type of strategy selected
        $(document).ready(function () {

            if (@ViewBag.VersionModalpop != "") {
                var VersionModalpop = @ViewBag.VersionModalpop;

                if (VersionModalpop == '1') {
                    $('#VersionModal').modal('show');
                }
                else {
                    $("#VersionModal").css('display', 'none');
                }
            }

        });

    </script>

    <script>

        $(".mobileNavMenuButton").click(function (e) {
            e.stopPropagation();
            $(".mobileNavMenuFlotingOptions").toggleClass("d-none");
        });

        $(function () {
            $(document).on("click", function (e) {
                if ($(e.target).is(".mobileNavMenuFlotingOptions") === false) {
                    $(".mobileNavMenuFlotingOptions").addClass("d-none");
                }
            });
        });

        $("#mobHamburger").click(function () {
            $("#sidebarbass").addClass("active");
            $(".closeIconHamburger").addClass("active");
        });
        $(".closeIconHamburger").click(function () {
            $("#sidebarbass").removeClass("active");
            $(".closeIconHamburger").removeClass("active");
            $("body").css("overflow-x", "auto");
        });

        $('.welcomeSlider')
            .not(".slick-intialized")
            .slick({
                slidesToShow: 1,
                slidesToScroll: 1,
                prevArrow: false,
                nextArrow: false,
                dots: true,
                autoplay: true,
                adaptiveHeight: true,
                accessibility: false,
            });
    </script>

