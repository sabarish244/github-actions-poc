    <main>
        <div class="d-flex">
            <div class="custom-sidebar-head d-none d-xl-block" id="sidebarBS">
            </div>
            <aside class="sidebar custom-sidebar d-none d-xl-block pt-3" id="sidebarbass">
                <div class="px-4 pt-3 pb-2 text-center">
                    <a href="~/?bas=233" tabindex="1" class="logo-header text-hide">
                        <img class="logo-label  pb-2 pl-2" alt="xplatform" width="180" src="../../img/Expleo_logo.svg">
                    </a>
                    <hr class=" mx-0 mt-4 mb-0" />
                </div>
                <!--Call sidebar navigation partial-->
                @RenderSection("NavSidebar", required: false)
                @*This Part Menu Footer added Build number,version date and help part*@
                <div class="footermenu px-4">
                    <hr class="mx-0 mt-0 mb-0" />
                    <div class="d-flex align-items-center sidebar-footer-version pt-2 py-2 text-center">
                        <div class="col-5" style="border-right: 1px solid #fff">
                            <div>
                                <button class="d-flex sidebar-footer-help align-items-center" onclick="ShowNonResourceItem('User_Guide');">
                                    <div>
                                        <img alt="help" src="../../img/icons/HelpVersion.svg" />
                                    </div>
                                    <div class="p-text pl-2">
                                        Help
                                    </div>
                                </button>
                            </div>
                        </div>
                        <div class="col-7 px-1 pl-3">
                            <button class="p-text sidebar-footer-version-hover" onclick="ShowNonResourceItem('help');" title="Build number: @ViewBag.BuildNo, Version date: @ViewBag.VersionDate">
                                Version <strong title="Build number: @ViewBag.BuildNo, Version date: @ViewBag.VersionDate">@ViewBag.VersionNo</strong>
                            </button>
                        </div>
                    </div>
                </div>
            </aside>
            <div class="closeIconHamburger">
                <img src="../../img/icons/close-icon.svg" alt="Expleo" />
            </div>
            @*resizable sidebar element*@
            <div id="resizableSideBar"></div>
            @*collapse sidebar element*@
            <div id="collapseSideBar">
                <svg width="15" height="25" viewBox="0 0 15 25" fill="none" xmlns="http://www.w3.org/2000/svg">
                    <path d="M1.16092 10.7913L1.16101 10.7913L10.6965 1.14266C10.6965 1.14266 10.6965 1.14266 10.6965 1.14266C11.4971 0.332533 12.803 0.324703 13.613 1.12545L1.16092 10.7913ZM1.16092 10.7913C0.761218 11.196 0.562731 11.7257 0.565841 12.2527C0.568948 12.7794 0.77316 13.3068 1.17777 13.707L1.17792 13.7071L10.8265 23.2423C11.6366 24.0433 12.9426 24.0358 13.7431 23.2252C14.5436 22.4153 14.5358 21.1097 13.726 20.3091L13.7259 20.309L5.54415 12.2234L13.6302 4.04181L13.6303 4.04172C14.4306 3.23157 14.423 1.92601 13.6131 1.12552L1.16092 10.7913Z" fill="#6946C6" stroke="#6946C6" />
                </svg>
            </div>
            <section class="flex-grow-1 mainDivForApps w-100">

                <div>
                    <partial name="_LoginPartial" />
                </div>

                <div>
                    <div class="container-fluid" id="containerfluid">
                        <partial name="_ConsentPartial" />
                        <!-- Begin Body -->
                        @RenderBody()
                        <!-- End Body -->
                    </div>
                </div>
                @*App not found screen*@
                <div id="divAppNotFound" class="d-none">
                    <div class="appNotFound text-center">
                        <img alt="app no found" src="../../img/appnotfound.svg">
                        <div class="h1-text">
                            App not found
                        </div>
                    </div>
                </div>
            </section>
        </div>
    </main>

    <div class="modal fade" id="LogoutModal" tabindex="-1" role="dialog" data-backdrop="static" data-keyboard="false" aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered modal-dialog-logout">
            <div class="modal-content" style="border-radius: 5px; overflow: hidden">
                <div class="modal-body logoutbnts p-sm-5 p-3" style="border-radius: 10px">
                    <div class="logoutCancelIcon">
                        <button class="text-hide" data-dismiss="modal">
